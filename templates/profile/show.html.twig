{% extends 'FrontTemplate.html.twig' %}

{% block title %}User{% endblock %}

{% block body %}
    <h1>User</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user.id }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ user.username }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ user.password }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ user.prenom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ user.adresse }}</td>
            </tr>
            <tr>
                <th>Cin</th>
                <td>{{ user.cin }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ user.telephone }}</td>
            </tr>
            <tr>
                <th>Rib</th>
                <td>{{ user.rib }}</td>
            </tr>
            <tr>
                <th>MatFiscal</th>
                <td>{{ user.MatFiscal }}</td>
            </tr>
            <tr>
                <th>NbrePoint</th>
                <td>{{ user.NbrePoint }}</td>
            </tr>
            <tr>
                <th>Image_name</th>
                <td>{{ user.imageName }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ user.email }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ user.nom }}</td>
            </tr>
        </tbody>
    </table>
    <a href="{{ path('app_profile_edit', {'id': user.id}) }}">edit</a>

{% endblock %}
